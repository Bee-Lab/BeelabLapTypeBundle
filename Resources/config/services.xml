<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="beelab_lap.form_type.lap" class="%beelab_lap.lap_form_type%" public="true">
            <tag name="form.type" alias="beelab_lap" />
        </service>
        <service id="beelab_lap.twig_extension" class="Beelab\LapTypeBundle\Twig\BeelabLapTypeTwigExtension" public="false">
            <tag name="twig.extension" />
        </service>
    </services>

</container>

{
    "name": "beelab/lap-type-bundle",
    "type": "symfony-bundle",
    "description": "Provide a Lap type for Symfony forms",
    "keywords": ["Symfony", "bundle", "form", "type", "lap"],
    "homepage": "https://github.com/bee-lab/",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "massimiliano.arione@bee-lab.net"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/symfony": "^2.8|^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Beelab\\LapTypeBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
